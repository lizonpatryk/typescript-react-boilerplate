{
  "name": "simple-react-typescript-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate made with React, ReactRouter, TypeScript and CSSModules with SASS",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "NODE_ENV=development webpack-dev-server --config webpack/webpack.config.dev.js --host 0.0.0.0",
    "lint": "concurrently \"npm:lint:lang:*\"",
    "lint:fix": "concurrently \"npm:lint:fix:*\"",
    "lint:lang:scss": "stylelint --syntax scss src/**/*.scss",
    "lint:lang:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "lint:fix:scss": "stylelint --syntax scss src/**/*.scss --fix",
    "lint:fix:ts": "tslint -c tslint.json 'src/**/*.{ts,tsx}' --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Patryk Lizo≈Ñ",
  "license": "ISC",
  "browsers": "last 2 versions",
  "dependencies": {
    "classnames": "^2.2.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/classnames": "^2.2.7",
    "@types/node": "^10.12.18",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "autoprefixer": "^9.4.4",
    "babel-loader": "^8.0.5",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "dart-sass": "^1.15.3",
    "dotenv-webpack": "^1.6.0",
    "fork-ts-checker-webpack-plugin": "^0.4.15",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "postcss-loader": "^3.0.0",
    "raw-loader": "^1.0.0",
    "react-hot-loader": "^4.6.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.4.4",
    "svg-url-loader": "^2.3.2",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "tslint-react-a11y": "^1.0.0",
    "typescript": "^3.2.2",
    "typestrict": "^1.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  }
}
